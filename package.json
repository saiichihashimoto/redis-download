    "tar": "5.0.5",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@stryker-mutator/babel-transpiler": "2.0.2",
